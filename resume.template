		body {
			font-size: 62.5%;
			font-family: 'Avenir LT W01_55 Roman1475520';
			color: #13242A;
			padding: 40px;
		}

		.nobreak {
			float: none;
			position: relative;
			page-break-inside: avoid !important;
			margin-top: 30px;
		}

		#contact {
			float: right;
			margin-top: 10px;
			text-align: right;
		}

		.picture {
			border-radius: 50%;
			margin: 0 auto;
		}

		a {
			color: #2197A8;
			text-decoration: none;
		}

		a:visited {
			color: #2197A8;
		}

		a:hover {
			color: #2197A8;
			text-decoration: underline;
		}

		.header {
			font-size: 1.4em;
			margin-bottom: 40px;
		}

		.profile {
			font-family: 'Avenir LT W01_55 Roman1475520';
			font-size: 1em;
			margin: 0;
		}

		.professional_titles {
			font-size: .75em;
		}

		.label {
			margin-top: 0;
			margin-bottom: 1em;
		}

		.date {
			display: table-cell;
			font-size: 1em;
			width: 150px;
		}

		.info {
			display: table-cell;
		}

		.fa {
			font-size: 1em;
			margin-right: 5px;
			width: 20px;
		}

		.left {
			font-size: 1em;
		}

		.main {
			margin: 0 auto;
		}

		h1 {
			font-size: 2em;
			margin: 0;
		}

		h2 {
			font-size: 1.4em;
			font-weight: bold;
			text-transform: uppercase;
		}

		h3 {
			font-size: 1.5em;
			margin-bottom: 0;
		}

		h4 {
			font-size: 1.3em;
			margin: 0;
			font-weight: normal;
		}

		.skills {
			margin-bottom: 10px;
		}

		.indent {
			margin-left: 20px;
		}

		p {
			font-family: 'Avenir LT W01_55 Roman1475520';
			margin: 0;
		}

		li {
			font-family: 'Avenir LT W01_55 Roman1475520';
			font-size: 1.5em;
		}

		.keywords {
			border: 1px solid #13242A;
			border-radius: 40px;
			padding: 2px 10px;
			font-size: 1.1em;
			margin-left: 10px;
			margin-bottom: 5px;
			display: inline-block;
		}

		blockquote {
			margin: 2em 10px;
			padding: 1em 10px;
			border-left: 10px solid #edf0f5;
			font-family: 'Avenir LT W01_55 Roman1475520';
			font-size: 1.5em;
		}

		blockquote .name {
			font-weight: bold;
			font-family: 'Avenir LT W01_55 Roman1475520';
		}

		.responsive-show {
			display: none;
		}

		.employment-gap {
			margin-top: 10px;
			margin-left: 150px;
		}

		section.background-info {
			margin-bottom: 10px;
		}

		@media (max-width: 1000px) {}

		@media (max-width: 500px) {
			.responsive-hidden {
				display: none;
				visibility: hidden;
			}
			.responsive-show {
				display: inline;
				visibility: visible;
			}
		}
	</style>
	<script src="//cdnjs.cloudflare.com/ajax/libs/respond.js/1.4.2/respond.min.js"></script>
